    "rollup-plugin-progress": "^0.4.0",
    "rollup-plugin-uglify": "^6.0.0"
  },
  "dependencies": {
    "clone": "^2.1.1",
    "core-js": "^2.5.0",
    "d3-ease": "^1.0.3",
    "d3-hierarchy": "^1.1.5",
    "prop-types": "^15.5.7"
